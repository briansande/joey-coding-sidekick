{
  "name": "joey-coding-sidekick",
  "displayName": "joey-coding-sidekick",
  "description": "A coding sidekick to keep you company",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "extensionDependencies": [
    "RooVeterinaryInc.roo-cline"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "joey-sidekick",
          "title": "Joey",
          "icon": "media/joey.svg"
        }
      ]
    },
    "views": {
      "joey-sidekick": [
        {
          "id": "joey.sidebar",
          "name": "Joey",
          "type": "webview",
          "contextualTitle": "Joey"
        }
      ]
    },
    "commands": [
      {
        "command": "joey-coding-sidekick.startTask",
        "title": "Joey: Start New Roocode Task"
      },
      {
        "command": "joey.showStats",
        "title": "Joey: Show Stats",
        "icon": "$(gear)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "joey.showStats",
          "when": "view == joey.sidebar",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}